TEST     START   1000       
FROM     STL     RETADR
         LDX     ZERO
         LDA     ZERO
LOOP     ADD     SECOP
         TIX     FIROP
         JLT     LOOP
         STA     RESULT
         JSUB    RINT
         LDL     RETADR
         RSUB
RINT     AND     LMASK
         DIV     SIXTEN
         OR      CASCII
RINT1    TD      OUTPUT
         JEQ     RINT1
         WD      OUTPUT
         LDA     RESULT
         AND     RMASK
         OR      CASCII
RINT2    TD      OUTPUT
         JEQ     RINT2
         WD      OUTPUT
         RSUB
ZERO     WORD    0
FIROP    WORD    7
SECOP    WORD    15
RESULT   RESW    1
RETADR   RESW    1
LMASK    WORD    -241
RMASK    WORD    -16
CASCII   WORD    48
SIXTEN   WORD    16
OUTPUT   BYTE    X'05'
TEST1    WORD    4097
TEST2    WORD    32760
TEST3    WORD    -3858
TEST4    BYTE    X'05DA'
TEST5    BYTE    X'05DA15'
TEST6    BYTE    C'O'
TEST7    BYTE    C'S1'
TEST8    BYTE    C'E9A'
TEST9    BYTE    X'FE'
         END     FROM